 <simpleType id="Entity_e" name="Entity_e">
  <annotation> <documentation> Defines all entity </documentation> </annotation>
  <restriction base="string">
   <!-- Auxiliary Enclosure -->
   <pattern value="[Aa][Uu][Xx][Ii][Ll][Ii][Aa][Rr][Yy][Ee][Nn][Cc][Ll][Oo][Ss][Uu][Rr][Ee]"/>
   <!-- Auxiliary Mount -->
   <pattern value="[Aa][Uu][Xx][Ii][Ll][Ii][Aa][Rr][Yy][Mm][Oo][Uu][Nn][Tt]"/>
   <!-- Auxiliary Imager -->
   <pattern value="[Aa][Uu][Xx][Ii][Ll][Ii][Aa][Rr][Yy][Ii][Mm][Aa][Gg][Ee][Rr]"/>
   <!-- Auxiliary Spectrograph -->
   <pattern value="[Aa][Uu][Xx][Ii][Ll][Ii][Aa][Rr][Yy][Ss][Pp][Ee][Cc][Tt][Rr][Oo][Gg][Rr][Aa][Pp][Hh]"/>
   <!-- Calibration Screen -->
   <pattern value="[Cc][Aa][Ll][Ii][Bb][Rr][Aa][Tt][Ii][Oo][Nn][Ss][Cc][Rr][Ee][Ee][Nn]"/>
   <!-- Calibration Illumination -->
   <pattern value="[Cc][Aa][Ll][Ii][Bb][Rr][Aa][Tt][Ii][Oo][Nn][Ii][Ll][Ll][Uu][Mm][Ii][Nn][Aa][Tt][Ii][Oo][Nn]"/>
   <!-- Calibration Photodiodes -->
   <pattern value="[Cc][Aa][Ll][Ii][Bb][Rr][Aa][Tt][Ii][Oo][Nn][Pp][Hh][Oo][Tt][Oo][Dd][Ii][Oo][Dd][Ee][Ss]"/>
   <!-- Calibration Spectrograph -->
   <pattern value="[Cc][Aa][Ll][Ii][Bb][Rr][Aa][Tt][Ii][Oo][Nn][Ss][Pp][Ee][Cc][Tt][Rr][Oo][Gg][Rr][Aa][Pp][Hh]"/>
   <!-- Collimated Beam Projector -->
   <pattern value="[Cc][Oo][Ll][Ll][Ii][Mm][Aa][Tt][Ee][Dd][Bb][Ee][Aa][Mm][Pp][Rr][Oo][Jj][Ee][Cc][Tt][Oo][Rr]"/>
   <!-- Visible All Sky Camera -->
   <pattern value="[Vv][Ii][Ss][Ii][Bb][Ll][Ee][Aa][Ll][Ll][Ss][Kk][Yy][Cc][Aa][Mm][Ee][Rr][Aa]"/>
   <!-- Infra-Red All Sky Camera -->
   <pattern value="[Ii][Nn][Ff][Rr][Aa][Rr][Ee][Dd][Aa][Ll][Ll][Ss][Kk][Yy][Cc][Aa][Mm][Ee][Rr][Aa]"/>
   <!-- DIMM -->
   <pattern value="[Dd][Ii][Mm][Mm]"/>
   <!-- Differential Image Motion Monitor -->
   <pattern value="[Dd][Ii][Ff][Ff][Ee][Rr][Ee][Nn][Tt][Ii][Aa][Ll][Ii][Mm][Aa][Gg][Ee][Mm][Oo][Tt][Ii][Oo][Nn][Mm][Oo][Nn][Ii][Tt][oO][Rr]"/>
   <!-- Dust Monitor -->
   <pattern value="[Dd][Uu][Ss][Tt][Mm][Oo][Nn][Ii][Tt][Oo][Rr]"/>
   <!-- Earthquake Detector -->
   <pattern value="[Ee][Aa][Rr][Tt][Hh][Qq][Uu][Aa][Kk][Ee][Dd][Ee][Tt][Ee][Cc][Tt][Oo][Rr]"/>
   <!-- Lightning Detector -->
   <pattern value="[Ll][Ii][Gg][Hh][Tt][Nn][Ii][Nn][Gg][Dd][Ee][Tt][Ee][Cc][Tt][Oo][Rr]"/>
   <!-- Camera -->
   <pattern value="[Cc][Aa][Mm][Ee][Rr][Aa]"/>
   <!-- Archiver -->
   <pattern value="[Aa][Rr][Cc][Hh][Ii][Vv][Ee][Rr]"/>
   <!-- Catchup Archiver -->
   <pattern value="[Cc][Aa][Tt][Cc][Hh][Uu][Pp][Aa][Rr][Cc][Hh][Ii][Vv][Ee][Rr]"/>
   <!-- Processing Cluster -->
   <pattern value="[Pp][Rr][Oo][Cc][Ee][Ss][Ss][Ii][Nn][Gg][Cc][Ll][Uu][Ss][Tt][Ee][Rr]"/>
   <!-- EFD Replicator -->
   <pattern value="[Ee][Ff][Dd][Rr][Ee][Pp][Ll][Ii][Cc][Aa][Tt][Oo][Rr]"/>
   <!-- Calibration Generator -->
   <pattern value="[Cc][Aa][Ll][Ii][Bb][Rr][Aa][Tt][Ii][Oo][Nn][Gg][Ee][Nn][Ee][Rr][Aa][Tt][Oo][Rr]"/>
   <!-- Air Conditioning -->
   <pattern value="[Aa][Ii][Rr][Cc][Oo][Nn][Dd][Ii][Tt][Ii][Oo][Nn][Ii][Nn][Gg]"/>
   <!-- Power Conditioning -->
   <pattern value="[Pp][Oo][Ww][Ee][Rr][Cc][Oo][Nn][Dd][Ii][Tt][Ii][Oo][Nn][Ii][Nn][Gg]"/>
   <!-- LSST Enclosure -->
   <pattern value="[Ll][Ss][Ss][Tt][Ee][Nn][Cc][Ll][Oo][Ss][Uu][Rr][Ee]"/>
   <!-- LSST Mount -->
   <pattern value="[Ll][Ss][Ss][Tt][Mm][Oo][Uu][Nn][Tt]"/>
   <!-- LSST Hexapod -->
   <pattern value="[Ll][Ss][Ss][Tt][Hh][Ee][Xx][Aa][Pp][Oo][Dd]"/>
   <!-- LSST Rotator -->
   <pattern value="[Ll][Ss][Ss][Tt][Rr][Oo][Tt][Aa][Tt][Oo][Rr]"/>
   <!-- LSST -->
   <pattern value="[Ll][Ss][Ss][Tt]"/>
   <!-- Calypso -->
   <pattern value="[Cc][Aa][Ll][Yy][Pp][Ss][Oo]"/>
  </restriction>
 </simpleType>

