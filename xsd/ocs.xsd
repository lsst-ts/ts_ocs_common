<?xml version="1.0" encoding="UTF-8"?>

<schema
  xmlns="http://www.w3.org/2001/XMLSchema"
  xmlns:ocs="http://project.lsst.org/ts"
  targetNamespace="http://project.lsst.org/ts"
  elementFormDefault="qualified"
  attributeFormDefault="unqualified"
  version="20160607">

 <annotation>
  <documentation>
   Description: OCS Schema for LSST
   Author(s):
    Philip N. Daly (pdaly@lsst.org)
   Change Record:
    15-Apr-2016: Original version (pdaly@lsst.org)
    07-Jun-2016: Decprecated "WaitForCompletion" attribute (pdaly@lsst.org)
    07-Jun-2016: Decprecated "RecordID" attribute (pdaly@lsst.org)
    07-Jun-2016: Deleted "abort" command from generic set (pdaly@lsst.org)
    07-Jun-2016: Deleted "stop" command from generic set (pdaly@lsst.org)
    07-Jun-2016: Added "abort" command to business logic set (pdaly@lsst.org)
    07-Jun-2016: Added "stop" command to business logic set (pdaly@lsst.org)
    07-Jun-2016: Added "Timeout" attribute (pdaly@lsst.org)
    07-Jun-2016: Added "waitForEvents" command to script set (pdaly@lsst.org)
   Validation Test:
    15-Apr-2016: Validated via validome.org and installed on project.lsst.org (pdaly@lsst.org)
    07-Jun-2016: Validated via validome.org and installed on project.lsst.org (pdaly@lsst.org)
   Copyright (c) 2016 AURA Inc. All rights reserved, Released under the GPL.
  </documentation>
 </annotation>

 <!--
  #+
  #  Section 1: Primitives
  #-
 -->

 <!-- boolean(s) -->

 <simpleType id="BeamBlock_b" name="BeamBlock_b">
  <annotation> <documentation> Beam blocker </documentation> </annotation>
  <restriction base="boolean"/>
 </simpleType>

 <simpleType id="Guider_b" name="Guider_b">
  <annotation> <documentation> Guide detector(s) state during an integration </documentation> </annotation>
  <restriction base="boolean"/>
 </simpleType>

 <simpleType id="Science_b" name="Science_b">
  <annotation> <documentation> Science detector(s) state during an integration </documentation> </annotation>
  <restriction base="boolean"/>
 </simpleType>

 <simpleType id="Shutter_b" name="Shutter_b">
  <annotation> <documentation> Shutter state during an integration </documentation> </annotation>
  <restriction base="boolean"/>
 </simpleType>

 <!-- DEPRECATED
  <simpleType id="WaitForCompletion_b" name="WaitForCompletion_b">
   <annotation> <documentation> Script wait for completion directive </documentation> </annotation>
   <restriction base="boolean"/>
  </simpleType>
 -->

 <simpleType id="Wfs_b" name="Wfs_b">
  <annotation> <documentation> Wavefront detector(s) state during an integration </documentation> </annotation>
  <restriction base="boolean"/>
 </simpleType>

 <!-- enumeration(s) -->

 <simpleType id="Calibration_e" name="Calibration_e">
  <annotation> <documentation> Defines calibration types (PLACEHOLDER) </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Nn][Oo][Nn][Ee]"/>
   <pattern value="[Tt][Bb][Dd]1"/>
   <pattern value="[Tt][Bb][Dd]2"/>
  </restriction>
 </simpleType>

 <simpleType id="Dec_e" name="Dec_e">
  <annotation> <documentation> Declination (\u00B190:00:00.000) </documentation> </annotation>
  <restriction base="string">
   <pattern value="[+-]?[0-9][0-9]:[012345][0-9]:[012345][0-9]\.[0-9]{3}"/>
  </restriction>
 </simpleType>

 <simpleType id="Dither_e" name="Dither_e">
  <annotation> <documentation> Telescope dither pattern (PLACEHOLDER) </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Nn][Oo][Nn][Ee]"/>
   <pattern value="[Tt][Bb][Dd]1"/>
   <pattern value="[Tt][Bb][Dd]2"/>
  </restriction>
 </simpleType>

 <simpleType id="Dome_e" name="Dome_e">
  <annotation> <documentation> Dome options (PLACEHOLDER) </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Nn][Oo][Nn][Ee]"/>
   <pattern value="[Tt][Bb][Dd]1"/>
   <pattern value="[Tt][Bb][Dd]2"/>
  </restriction>
 </simpleType>

 <simpleType id="Disperser_e" name="Disperser_e">
  <annotation> <documentation> Known dispersers </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Rr][Ee][Dd]"/>
   <pattern value="[Gg][Rr][Ee][Ee][Nn]"/>
   <pattern value="[Bb][Ll][Uu][Ee]"/>
  </restriction>
 </simpleType>

 <simpleType id="Error_e" name="Error_e">
  <annotation> <documentation> Script error directive </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Aa][Bb][Oo][Rr][Tt]"/>
   <pattern value="[Cc][Oo][Nn][Tt][Ii][Nn][Uu][Ee]"/>
  </restriction>
 </simpleType>

 <simpleType id="Filter_e" name="Filter_e">
  <annotation> <documentation> Known filters </documentation> </annotation>
  <restriction base="string">
   <pattern value="[ugriyz]"/>
  </restriction>
 </simpleType>

 <simpleType id="LaserLightSource_e" name="LaserLightSource_e">
  <annotation> <documentation> Laser light source </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Ll][Aa][Ss][Ee][Rr][Ss][Oo][Uu][Rr][Cc][Ee][Aa]"/>
   <pattern value="[Ll][Aa][Ss][Ee][Rr][Ss][Oo][Uu][Rr][Cc][Ee][Bb]"/>
   <pattern value="[Ll][Aa][Ss][Ee][Rr][Ss][Oo][Uu][Rr][Cc][Ee][Cc]"/>
  </restriction>
 </simpleType>

 <simpleType id="LightPosition_e" name="LightPosition_e">
  <annotation> <documentation> Light position </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Cc][Bb][Pp]"/>
   <pattern value="[Ss][Cc][Rr][Ee][Ee][Nn]"/>
  </restriction>
 </simpleType>

 <simpleType id="LightSource_e" name="LightSource_e">
  <annotation> <documentation> Light source </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Ll][Aa][Ss][Ee][Rr]"/>
   <pattern value="[Ww][Hh][Ii][Tt][Ee]"/>
  </restriction>
 </simpleType>

 <simpleType id="Observation_e" name="Observation_e">
  <annotation> <documentation> Script observation type </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Bb][Ii][Aa][Ss]"/>
   <pattern value="[Zz][Ee][Rr][Oo]"/>
   <pattern value="[Dd][Aa][Rr][Kk]"/>
   <pattern value="[Ff][Ll][Aa][Tt]"/>
   <pattern value="[Oo][Bb][Jj][Ee][Cc][Tt]"/>
   <pattern value="[Te][Ee][Ss][Tt]"/>
  </restriction>
 </simpleType>

 <simpleType id="OnOff_e" name="OnOff_e">
  <annotation> <documentation> anything that can be defined as on or off </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Oo][Nn]"/>
   <pattern value="[Oo][Ff][Ff]"/>
  </restriction>
 </simpleType>

 <simpleType id="OpenClosed_e" name="OpenClosed_e">
  <annotation> <documentation> anything that can be defined as open or closed </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Oo][Pp][Ee][Nn]"/>
   <pattern value="[Cc][Ll][Oo][Ss][Ee][Dd]"/>
  </restriction>
 </simpleType>

 <simpleType id="Optics_e" name="Optics_e">
  <annotation> <documentation> Optics options (PLACEHOLDER) </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Nn][Oo][Nn][Ee]"/>
   <pattern value="[Tt][Bb][Dd]1"/>
   <pattern value="[Tt][Bb][Dd]2"/>
  </restriction>
 </simpleType>

 <simpleType id="ParkedUnparked_e" name="ParkedUnparked_e">
  <annotation> <documentation> anything that can be defined as parked or unparked </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Pp][Aa][Rr][Kk][Ee][Dd]"/>
   <pattern value="[Uu][Nn][Pp][Aa][Rr][Kk][Ee][Dd]"/>
  </restriction>
 </simpleType>

 <simpleType id="Position_e" name="Position_e">
  <annotation> <documentation> Calibration screen position </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Nn][Oo][Rr][Mm][Aa][Ll]"/>
   <pattern value="[Pp][Aa][Rr][Kk]"/>
   <pattern value="[+-]?[0-9]*\.[0-9]*"/>
  </restriction>
 </simpleType>

 <simpleType id="RA_e" name="RA_e">
  <annotation> <documentation> Right ascension (00:00:00.000 \u2264 RA \u2264 +23:59:59.999) </documentation> </annotation>
  <restriction base="string">
   <pattern value="[012]?[0123]:[012345][0-9]:[012345][0-9]\.[0-9]{3}"/>
  </restriction>
 </simpleType>

 <simpleType id="Recipe_e" name="Recipe_e">
  <annotation> <documentation> Script recipe type </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Ss][Tt][Aa][Nn][Dd][Aa][Rr][Dd][Vv][Ii][Ss][Ii][Tt]"/>
  </restriction>
 </simpleType>

 <simpleType id="ScreenPosition_e" name="ScreenPosition_e">
  <annotation> <documentation> Screen position as a string </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Nn][Oo][Rr][Mm][Aa][Ll]"/>
   <pattern value="[Pp][Aa][Rr][Kk]"/>
  </restriction>
 </simpleType>

 <simpleType id="ScriptSource_e" name="ScriptSource_e">
  <annotation> <documentation> Identifying source of script </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Tt][Ee][Mm][Pp][Ll][Aa][Tt][Ee]"/>
   <pattern value="[Ww][Ee][Bb][Ff][Oo][Rr][Mm]"/>
   <pattern value="[Ss][Cc][Hh][Ee][Dd][Uu][Ll][Ee][Rr]"/>
  </restriction>
 </simpleType>

 <simpleType id="Track_e" name="Track_e">
  <annotation> <documentation> Track options (PLACEHOLDER) </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Nn][Oo][Nn][Ee]"/>
   <enumeration value="None"/>
   <pattern value="[Tt][Bb][Dd]1"/>
   <pattern value="[Tt][Bb][Dd]2"/>
  </restriction>
 </simpleType>

 <simpleType id="WhiteLightSource_e" name="WhiteLightSource_e">
  <annotation> <documentation> White light source </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Ww][Hh][Ii][Tt][Ee][Ss][Oo][Uu][Rr][Cc][Ee][Aa]"/>
   <pattern value="[Ww][Hh][Ii][Tt][Ee][Ss][Oo][Uu][Rr][Cc][Ee][Bb]"/>
   <pattern value="[Ww][Hh][Ii][Tt][Ee][Ss][Oo][Uu][Rr][Cc][Ee][Cc]"/>
  </restriction>
 </simpleType>

 <simpleType id="WrapDir_e" name="WrapDir_e">
  <annotation> <documentation> Wrap direction (clockwise or counter-clockwise) </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Cc][Ww]"/>
   <pattern value="[Cc][Cc][Ww]"/>
  </restriction>
 </simpleType>

 <!-- simple type(s) -->

 <simpleType id="Altitude_t" name="Altitude_t">
  <annotation> <documentation> Telescope altitude (0.0\u00B0 \u2264 altitude \u2264 +90.0\u00B0) </documentation> </annotation>
  <restriction base="double">
   <minExclusive value="0.0"/>
   <maxExclusive value="90.0"/>
  </restriction>
 </simpleType>
 
 <simpleType id="Azimuth_t" name="Azimuth_t">
  <annotation> <documentation> Telescope azimuth (\u00B1360.0\u00B0, N=0\u00B0, E=90\u00B0, S=180\u00B0, W=270\u00B0) </documentation> </annotation>
  <restriction base="double">
   <minInclusive value="-360.0"/>
   <maxInclusive value="360.0"/>
  </restriction>
 </simpleType>

 <simpleType id="Elevation_t" name="Elevation_t">
  <annotation> <documentation> Telescope elevation (0.0\u00B0 \u2264 elevation \u2264 +90.0\u00B0) </documentation> </annotation>
  <restriction base="double">
   <minExclusive value="0.0"/>
   <maxExclusive value="90.0"/>
  </restriction>
 </simpleType>
 
 <simpleType id="Equinox_t" name="Equinox_t">
  <annotation> <documentation> Co-ordinate system equinox (1950.0 \u2264 equinox \u2264 2050.0) </documentation> </annotation>
  <restriction base="double">
   <minInclusive value="1950.0"/>
   <maxInclusive value="2050.0"/>
  </restriction>
 </simpleType>

 <simpleType id="Focus_t" name="Focus_t">
  <annotation> <documentation> Focus value (-100.0\u03BCm \u2264 focus \u2264 +100.0\u03BCm) </documentation> </annotation>
  <restriction base="double">
   <minInclusive value="-100.0"/>
   <maxInclusive value="100.0"/>
  </restriction>
 </simpleType>

 <simpleType id="M3Angle_t" name="M3Angle_t">
  <annotation> <documentation> M3 angle (-25.0\u00B0 \u2264 angle \u2264 +25.0\u00B0) </documentation> </annotation>
  <restriction base="double">
   <minInclusive value="-25.0"/>
   <maxInclusive value="25.0"/>
  </restriction>
 </simpleType>
 
 <simpleType id="NegativeDouble_t" name="NegativeDouble_t">
  <annotation> <documentation> Defines a double \u003C 0.0 </documentation> </annotation>
  <restriction base="double">
   <maxExclusive value="0.0"/>
  </restriction>
 </simpleType>

 <simpleType id="PerCentDouble_t" name="PerCentDouble_t">
  <annotation> <documentation> Defines a percentage (0.0 \u2264 percent \u2264 +100.0) </documentation> </annotation>
  <restriction base="double">
   <minInclusive value="0.0"/>
   <maxInclusive value="100.0"/>
  </restriction>
 </simpleType>

 <simpleType id="PositiveDouble_t" name="PositiveDouble_t">
  <annotation> <documentation> Defines a double (\u2265 0.0) </documentation> </annotation>
  <restriction base="double">
   <minInclusive value="0.0"/>
  </restriction>
 </simpleType>

 <simpleType id="PositiveLong_t" name="PositiveLong_t">
  <annotation> <documentation> Defines a long (\u2265 0) </documentation> </annotation>
  <restriction base="long">
   <minInclusive value="0"/>
  </restriction>
 </simpleType>

 <simpleType id="RotatorAngle_t" name="RotatorAngle_t">
  <annotation> <documentation> Rotator angle (-270.0\u00B0 \u2264 angle \u2264 +270.0\u00B0) </documentation> </annotation>
  <restriction base="double">
   <minInclusive value="-270.0"/>
   <maxInclusive value="270.0"/>
  </restriction>
 </simpleType>
 
 <simpleType id="ScreenPosition_t" name="ScreenPosition_t">
  <annotation> <documentation> Screen position as an angle (-90.0\u00B0 \u2264 position \u2264 +90.0\u00B0) </documentation> </annotation>
  <restriction base="double">
   <minInclusive value="-90.0"/>
   <maxInclusive value="90.0"/>
  </restriction>
 </simpleType>

 <simpleType id="TiltAngle_t" name="TiltAngle_t">
  <annotation> <documentation> Tilt angle (-90.0\u00B0 \u2264 angle \u2264 +90.0\u00B0) </documentation> </annotation>
  <restriction base="double">
   <minInclusive value="-90.0"/>
   <maxInclusive value="90.0"/>
  </restriction>
 </simpleType>
 
 <simpleType id="TipAngle_t" name="TipAngle_t">
  <annotation> <documentation> Tip angle (-90.0\u00B0 \u2264 angle \u2264 +90.0\u00B0) </documentation> </annotation>
  <restriction base="double">
   <minInclusive value="-90.0"/>
   <maxInclusive value="90.0"/>
  </restriction>
 </simpleType>
 
 <simpleType id="WavelengthDouble_t" name="WavelengthDouble_t">
  <annotation> <documentation> Defines a wavelength (3000.0\u212B \u2264 \u03BB \u2264 10000.0\u212B) </documentation> </annotation>
  <restriction base="double">
   <minInclusive value="3000.0"/>
   <maxInclusive value="10000.0"/>
  </restriction>
 </simpleType>

 <!-- complex type(s) -->

 <complexType id="AltAz_t" name="AltAz_t">
  <annotation> <documentation> Alt and Az expressed from simple types </documentation> </annotation>
  <all>
   <element name="Altitude" type="ocs:Altitude_t" minOccurs="1" maxOccurs="1"/>
   <element name="Azimuth" type="ocs:Azimuth_t" minOccurs="1" maxOccurs="1"/>
  </all>
 </complexType>

 <complexType id="Dither_t" name="Dither_t">
  <annotation> <documentation> Dither pattern expressed from simple types (PLACEHOLDER) </documentation> </annotation>
  <all>
   <element name="ra_offset" type="float" minOccurs="1" maxOccurs="1"/>
   <element name="dec_offset" type="float" minOccurs="1" maxOccurs="1"/>
   <element name="pattern" type="ocs:Dither_e" minOccurs="1" maxOccurs="1"/>
  </all>
 </complexType>

 <complexType id="RADec_t" name="RADec_t">
  <annotation> <documentation> RA, Dec and Equinox expressed from simple and complex types </documentation> </annotation>
  <all>
   <element name="RightAscension" type="ocs:RA_e" minOccurs="1" maxOccurs="1"/>
   <element name="Declination" type="ocs:Dec_e" minOccurs="1" maxOccurs="1"/>
   <element name="Equinox" type="ocs:Equinox_t" minOccurs="1" maxOccurs="1"/>
  </all>
 </complexType>

 <complexType id="ScanDuration_t" name="ScanDuration_t">
  <annotation> <documentation> cimsScanDiode duration parameters </documentation> </annotation>
  <all>
   <element name="DiodeIDs" type="string" minOccurs="0" maxOccurs="1"/>
   <element name="Frequency" type="ocs:PositiveDouble_t" minOccurs="0" maxOccurs="1"/>
   <element name="Duration" type="ocs:PositiveDouble_t" minOccurs="0" maxOccurs="1"/>
  </all>
 </complexType>

 <complexType id="ScanUntil_t" name="ScanUntil_t">
  <annotation> <documentation> cimsScanDiode until parameters </documentation> </annotation>
  <all>
   <element name="DiodeIDs" type="string" minOccurs="0" maxOccurs="1"/>
   <element name="Frequency" type="ocs:PositiveDouble_t" minOccurs="0" maxOccurs="1"/>
   <element name="Duration" type="ocs:NegativeDouble_t" minOccurs="0" maxOccurs="1"/>
   <element name="Until" type="ocs:PositiveDouble_t" minOccurs="0" maxOccurs="1"/>
  </all>
 </complexType>

 <!--
  #+
  #  Section 2: Entities
  #-
 -->

 <!-- Entitys (patterns) -->

 <simpleType id="AuxiliaryEnclosure_e" name="AuxiliaryEnclosure_e">
  <annotation> <documentation> auxiliary enclosure entity </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Aa][Uu][Xx][Ii][Ll][Ii][Aa][Rr][Yy][Ee][Nn][Cc][Ll][Oo][Ss][Uu][Rr][Ee]"/>
  </restriction>
 </simpleType>

 <simpleType id="AuxiliaryMount_e" name="AuxiliaryMount_e">
  <annotation> <documentation> auxiliary mount entity </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Aa][Uu][Xx][Ii][Ll][Ii][Aa][Rr][Yy][Mm][Oo][Uu][Nn][Tt]"/>
  </restriction>
 </simpleType>

 <simpleType id="AuxiliaryImager_e" name="AuxiliaryImager_e">
  <annotation> <documentation> auxiliary imager entity </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Aa][Uu][Xx][Ii][Ll][Ii][Aa][Rr][Yy][Ii][Mm][Aa][Gg][Ee][Rr]"/>
  </restriction>
 </simpleType>

 <simpleType id="AuxiliarySpectrograph_e" name="AuxiliarySpectrograph_e">
  <annotation> <documentation> auxiliary spectrograph entity </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Aa][Uu][Xx][Ii][Ll][Ii][Aa][Rr][Yy][Ss][Pp][Ee][Cc][Tt][Rr][Oo][Gg][Rr][Aa][Pp][Hh]"/>
  </restriction>
 </simpleType>

 <simpleType id="CalibrationScreen_e" name="CalibrationScreen_e">
  <annotation> <documentation> calibration screen entity </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Cc][Aa][Ll][Ii][Bb][Rr][Aa][Tt][Ii][Oo][Nn][Ss][Cc][Rr][Ee][Ee][Nn]"/>
  </restriction>
 </simpleType>

 <simpleType id="CalibrationIllumination_e" name="CalibrationIllumination_e">
  <annotation> <documentation> calibration illumination entity </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Cc][Aa][Ll][Ii][Bb][Rr][Aa][Tt][Ii][Oo][Nn][Ii][Ll][Ll][Uu][Mm][Ii][Nn][Aa][Tt][Ii][Oo][Nn]"/>
  </restriction>
 </simpleType>

 <simpleType id="CalibrationPhotodiodes_e" name="CalibrationPhotodiodes_e">
  <annotation> <documentation> calibration photodiodes entity </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Cc][Aa][Ll][Ii][Bb][Rr][Aa][Tt][Ii][Oo][Nn][Pp][Hh][Oo][Tt][Oo][Dd][Ii][Oo][Dd][Ee][Ss]"/>
  </restriction>
 </simpleType>

 <simpleType id="CalibrationSpectrograph_e" name="CalibrationSpectrograph_e">
  <annotation> <documentation> calibration spectrograph entity </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Cc][Aa][Ll][Ii][Bb][Rr][Aa][Tt][Ii][Oo][Nn][Ss][Pp][Ee][Cc][Tt][Rr][Oo][Gg][Rr][Aa][Pp][Hh]"/>
  </restriction>
 </simpleType>

 <simpleType id="CollimatedBeamProjector_e" name="CollimatedBeamProjector_e">
  <annotation> <documentation> collimated beam projector entity </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Cc][Oo][Ll][Ll][Ii][Mm][Aa][Tt][Ee][Dd][Bb][Ee][Aa][Mm][Pp][Rr][Oo][Jj][Ee][Cc][Tt][Oo][Rr]"/>
  </restriction>
 </simpleType>

 <simpleType id="VisibleAllSkyCamera_e" name="VisibleAllSkyCamera_e">
  <annotation> <documentation> visible all sky camera entity </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Vv][Ii][Ss][Ii][Bb][Ll][Ee][Aa][Ll][Ll][Ss][Kk][Yy][Cc][Aa][Mm][Ee][Rr][Aa]"/>
  </restriction>
 </simpleType>

 <simpleType id="InfraRedAllSkyCamera_e" name="InfraRedAllSkyCamera_e">
  <annotation> <documentation> infra-red all sky camera entity </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Ii][Nn][Ff][Rr][Aa][Rr][Ee][Dd][Aa][Ll][Ll][Ss][Kk][Yy][Cc][Aa][Mm][Ee][Rr][Aa]"/>
  </restriction>
 </simpleType>

 <simpleType id="DIMM_e" name="DIMM_e">
  <annotation> <documentation> differential image motion monitor entity </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Dd][Ii][Mm][Mm]"/>
   <pattern value="[Dd][Ii][Ff][Ff][Ee][Rr][Ee][Nn][Tt][Ii][Aa][Ll][Ii][Mm][Aa][Gg][Ee][Mm][Oo][Tt][Ii][Oo][Nn][Mm][Oo][Nn][Ii][Tt][oO][Rr]"/>
  </restriction>
 </simpleType>

 <simpleType id="DustMonitor_e" name="DustMonitor_e">
  <annotation> <documentation> dust monitor entity </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Dd][Uu][Ss][Tt][Mm][Oo][Nn][Ii][Tt][Oo][Rr]"/>
  </restriction>
 </simpleType>

 <simpleType id="EarthquakeDetector_e" name="EarthquakeDetector_e">
  <annotation> <documentation> earthquake detector entity </documentation> </annotation>
  <restriction base="string">
   <pattern value="[Ee][Aa][Rr][Tt][Hh][Qq][Uu][Aa][Kk][Ee][Dd][Ee][Tt][Ee][Cc][Tt][Oo][Rr]"/>
  </restriction>
 </simpleType>

 <simpleType id="LightningDetector_e" name="LightningDetector_e">
  <annotation> <documentation> lightning detector entity </documentation> </annotation>
  <restriction base="string">
