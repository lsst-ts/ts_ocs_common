  <ocs:ccsInitGuiders Entity="Camera">
   <ocs:RoiSpec>CcsRoiSpec</ocs:RoiSpec>
  </ocs:ccsInitGuiders>
 </ocs:cameraCommandSequence>

 <!-- wait for an event -->
